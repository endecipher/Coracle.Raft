{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Coracle": {
    "LogLevel" :  "Debug",
    "EngineConfiguration": {
      "DiscoveryServerUri": "https://localhost:7016/",
      "EventProcessorQueueSize": 100,
      "EventProcessorWaitTimeWhenQueueEmpty_InMilliseconds": 1000,
      "ThisNodeUri": null,
      "NodeId": "Node1",
      "SendAppendEntriesRPC_MaxRetryInfinityCounter": 500,
      "SendRequestVoteRPC_MaxRetryInfinityCounter": 500,
      "SendAppendEntriesRPC_MaxSessionCapacity": 50,
      "IncludeOriginalClientCommandInResults": true,
      "WaitPostEnroll_InMilliseconds": 1000,
      "MaxElectionTimeout_InMilliseconds": 30000,
      "MinElectionTimeout_InMilliseconds": 20000,
      "HeartbeatInterval_InMilliseconds": 300000,
      "NoLeaderElectedWaitInterval_InMilliseconds": 10000,
      "ClientCommandTimeout_InMilliseconds": 300000,
      "AppendEntriesTimeoutOnReceive_InMilliseconds": 600000,
      "RequestVoteTimeoutOnReceive_InMilliseconds": 600000,
      "RequestVoteTimeoutOnSend_InMilliseconds": 600000,
      "AppendEntriesTimeoutOnSend_InMilliseconds": 600000
    }    
  }
}
